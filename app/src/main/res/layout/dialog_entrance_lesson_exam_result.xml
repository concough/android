<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorWhite"
    android:layoutDirection="ltr">

    <TextView
        android:id="@+id/DELEResult_titleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/entrance_exam_result_title"
        android:textColor="@color/colorBlack"
        android:textSize="14sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        android:textDirection="rtl"
        />

    <TextView
        android:id="@+id/DELEResult_subTitleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorBlack"
        android:textSize="15sp"
        android:text=""
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_titleTextView"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:textDirection="rtl"
        android:gravity="center"
        android:textAlignment="center"
        />

    <LinearLayout
        android:id="@+id/DELEResult_section1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="top"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_subTitleTextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:layout_marginRight="32dp"
            android:layout_marginEnd="32dp"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/DELEResult_questionCountImageView"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:src="@drawable/question_80"
                app:srcCompat="@drawable/question_80"
                android:layout_marginBottom="4dp"
                />

            <TextView
                android:id="@+id/DELEResult_questionCountTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/colorConcoughBlue"
                android:textSize="14sp"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/DELEResult_examTimeImageView"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:src="@drawable/timer2_100"
                app:srcCompat="@drawable/timer2_100"
                android:layout_marginBottom="4dp"
                />

            <TextView
                android:id="@+id/DELEResult_examTimeTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/colorConcoughBlue"
                android:textSize="14sp"
                />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/DELEResult_linearLine1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_section1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colorConcoughGray4" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/DELEResult_section2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_linearLine1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp">

        <TextView
            android:id="@+id/DELEResult_resultExamTimeTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorBlack"
            android:textSize="15sp"
            android:text=""
            android:layout_marginRight="8dp"
            android:layout_marginEnd="8dp"
            />

        <TextView
            android:id="@+id/DELEResult_resultExamTimeLabelTextView"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:textColor="@color/colorConcoughGray"
            android:textSize="13sp"
            android:text="@string/entrance_exam_result_time_label"
            android:textDirection="rtl"
            android:textAlignment="textEnd"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/DELEResult_section3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_section2"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp"
        >

        <TextView
            android:id="@+id/DELEResult_resultExamPercentageTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorBlack"
            android:textSize="17sp"
            android:text=""
            android:layout_marginRight="8dp"
            android:layout_marginEnd="8dp"
            />

        <TextView
            android:id="@+id/DELEResult_resultExamPercentageLabelTextView"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:textColor="@color/colorConcoughGray"
            android:textSize="13sp"
            android:text="@string/entrance_exam_result_percentage_label"
            android:textDirection="rtl"
            android:textAlignment="textEnd"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/DELEResult_section4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="top"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_section3"
        android:layout_marginTop="24dp"
        >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_marginRight="32dp"
            android:layout_marginEnd="32dp">
            <ImageView
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:src="@drawable/checked_checkbox_true_128"
                app:srcCompat="@drawable/checked_checkbox_true_128"
                android:layout_marginBottom="8dp"
                />

            <TextView
                android:id="@+id/DELEResult_trueAnswerTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorConcoughGreen"
                android:textSize="15sp"
                android:text=""
                android:textDirection="rtl"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_marginRight="32dp"
            android:layout_marginEnd="32dp">
            <ImageView
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:src="@drawable/close_window_false_128"
                app:srcCompat="@drawable/close_window_false_128"
                android:layout_marginBottom="8dp"
                />

            <TextView
                android:id="@+id/DELEResult_falseAnswerTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorConcoughRedLight"
                android:textSize="15sp"
                android:text=""
                android:textDirection="rtl"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center">

            <ImageView
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:src="@drawable/unchecked_checkbox_128"
                app:srcCompat="@drawable/unchecked_checkbox_128"
                android:layout_marginBottom="8dp"
                />

            <TextView
                android:id="@+id/DELEResult_noAnswerTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorConcoughGray2"
                android:textSize="15sp"
                android:text=""
                android:textDirection="rtl"
                />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/DELEResult_linearLine2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_section4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colorConcoughGray3" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/DELEResult_section5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/DELEResult_linearLine2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        >

        <Button
            android:id="@+id/DELEResult_closeButton"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="@string/close"
            android:textColor="@color/colorConcoughGray2"
            android:background="@drawable/concough_border_radius_gray_style"
            android:textSize="15sp"
            android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp"
            />
        <Button
            android:id="@+id/DELEResult_seeResultsButton"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="@string/entrance_exam_result_title"
            android:textColor="@color/colorConcoughBlue"
            android:background="@drawable/concough_border_radius_style"
            android:textSize="15sp"
            />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>